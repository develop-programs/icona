# Icon Source Configuration
# Choose one: local file system or remote GitHub repository

# Local icons (if using local file system)
ICONS_SOURCE=local
ICONS_LOCAL_PATH=./icons

# Remote icons (GitHub repository)
# ICONS_SOURCE=remote  
# ICONS_REMOTE_URL=https://api.github.com/repos/feathericons/feather/contents/icons
# ICONS_AUTH_TOKEN=your_github_token_here
# ICONS_TIMEOUT=30000

# Build Configuration
BUILD_OUTPUT_DIR=./dist
BUILD_REACT=true
BUILD_VUE=true
BUILD_REACT_NATIVE=true

# Optimization
ENABLE_SVG_OPTIMIZATION=true

# NPM Publishing Token (for maintainers)
# Get from: https://www.npmjs.com/settings/tokens
NPM_TOKEN=your_npm_access_token_here

# Environment
NODE_ENV=development